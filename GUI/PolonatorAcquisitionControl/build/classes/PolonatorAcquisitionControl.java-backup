// TAKEN FROM:
// http://forums.sun.com/thread.jspa?messageID=9800946
private SwingWorker<Integer, String> createWorker(final String execCommand, final JTextArea outputWindow, final String appTab){
    return new SwingWorker<Integer, String>(){
 
        int idealSize = 3000;
        int maxExcess = 500;
        int excess;
 
        @Override
        protected Integer doInBackground() throws Exception{
            String line;
            int read = -1;
            String path = null;
            String cmdPlusPath;
            String cmdArgsArray[];
            
            if(appTab.equals("flUtils")){
                System.out.println("fl");
                disableAllFlUtilButtons();
            }
            
            // parse path out of execCommand so the command is started from the
            // correct directory
            // execCommand can contain the following:
            // a 'command' with args
            // a 'command' with no args
            // a 'command plus path' with args
            // a 'command plus path' with no args
            //
            // first, does it have args?  if so, parse out just the command
            // into cmdPlusPath; also, split the full command + any args
            // into an array
            cmdPlusPath = execCommand;
            if(execCommand.indexOf(" ") != -1){
                cmdPlusPath = execCommand.substring(0, execCommand.indexOf(" ")+1);
            }
            cmdArgsArray = execCommand.split(" ");

            // now parse out the path, if it exists
            if(cmdPlusPath.lastIndexOf("/") != -1){
                path = cmdPlusPath.substring(0, cmdPlusPath.lastIndexOf("/")+1);
            }
            
            System.out.println("For command " + execCommand);
            System.out.println(" cmdPlusPath = " + cmdPlusPath);
            System.out.println(" path = " + path);
/**/            
            Process p = Runtime.getRuntime().exec(cmdArgsArray, null, new File(path));
            InputStream is = p.getInputStream();
            OutputStream os = p.getOutputStream();
            StringBuilder sb = new StringBuilder();

            outputWindow.setText("");
            
            while((read=is.read())!=-1){
                sb.append((char)read);
                if(read=='\n'){
                    outputWindow.append(sb.toString());
                    outputWindow.setCaretPosition(outputWindow.getDocument().getLength());
                    excess = outputWindow.getDocument().getLength() - idealSize;
                    if(excess >= maxExcess){
                        outputWindow.replaceRange("", 0, excess);
                    }
                    sb.setLength(0);
                }
            }
            is.close();
 
            try{
                p.waitFor();
            } catch (InterruptedException e){
                System.err.println("Error: waiting for " + execCommand + " to terminate: " + e.getMessage());
            }
            /**/
            return new Integer(p.exitValue());
        }
        
        @Override
        protected void done(){
            if(appTab.equals("flUtils")){
                enableAllFlUtilButtons();
            }
            else if(appTab.equals("acqUtils")){
                enableAllAcqUtilButtons();
            }
            else if(appTab.equals("acqUtilsLive")){
                enableAllAcqUtilButtons();
                utilFocusBar.setEnabled(false);
                utilSetFocus.setEnabled(false);
            }
            else if(appTab.equals("focus")){
                try{
                    Integer i = get();
                    utilFocusLabel.setText(i.toString());
                    utilFocusBar.setValue(i);
                } catch (Exception ignore){}
            }
            else if(appTab.equals("acq")){
                jf.setDefaultCloseOperation(DO_NOTHING_ON_CLOSE);
                enableALLButtons();
            }
            else if(appTab.equals("polonatorStart")){
                enableALLButtons();
                polonatorCycleEntry.setEnabled(true);
                polonatorCycleEntryValidate.setEnabled(true);
                polonatorTouch.setEnabled(true);
                jf.setDefaultCloseOperation(EXIT_ON_CLOSE);
            }
        }
    };
}

    
    
    
